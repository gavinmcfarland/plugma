{
	"private": true,
	"type": "module",
	"scripts": {
		"dev": "plugma dev",
		"build": "plugma build",
		"preview": "plugma preview",
		"release": "plugma release"
	},
	"dependencies": {
		"vue": "^3.5.12"
	},
	"devDependencies": {
		"@vitejs/plugin-vue": "^5.1.4",
		"typescript": "~5.6.2",
		"vite": "^5.4.10",
		"vue-tsc": "^2.1.8",
		"@figma/plugin-typings": "^1.88.0",
		"plugma": "^<%- versions.plugmaNext %>"
	},
	"plugma": {
		"manifest": {
			"id": "<%- name %>-replace",
			"name": "<%- name %>",
			"main": "src/main.ts",
			"ui": "src/ui.ts",
			"editorType": [
				"figma",
				"figjam"
			],
			"networkAccess": {
				"allowedDomains": [
					"none"
				],
				"devAllowedDomains": [
					"http://localhost:*",
					"ws://localhost:9001"
				]
			}
		}
	}
}
