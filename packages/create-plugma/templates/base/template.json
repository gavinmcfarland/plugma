{
	"merge": {
		"**/*.json": {
			"strategy": "deep"
		}
	},
	"data": {
		"mainDir": "src/main",
		"uiDir": "src/ui"
	},
	"include": [
		{
			"source": "../frameworks/[framework]/components[hasUI]",
			"target": "src/ui/components"
		},
		{
			"source": "../frameworks/[framework]/assets[hasUI]",
			"target": "src/ui/assets"
		},
		{
			"source": "../typescript[typescript]/tsconfig.main.json",
			"target": "src/main/tsconfig.json"
		},
		{
			"source": "../frameworks/[framework]/[typescript]/tsconfig.ui.json[hasUI]",
			"target": "src/ui/tsconfig.json"
		},
		{
			"source": "../typescript[typescript]/tsconfig.ui.json[hasUI]",
			"target": "src/ui/tsconfig.json"
		}
	]
}
