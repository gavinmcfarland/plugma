{
	"private": true,
	"type": "module",
	"scripts": {
		"dev": "plugma dev",
		"build": "plugma build",
		"release": "plugma release"
	},
	"devDependencies": {
		"vite": "^5.4.10",
		"@figma/plugin-typings": "^1.100.2",
		"plugma": "^<%- versions.plugma %>"
	},
	"plugma": {
		"manifest": {
			"id": "<%- plugin.name %>",
			"name": "<%- plugin.name %>",
			"main": "<%- manifest.main %>",
			"ui": "<%- manifest.ui %>",
			"editorType": [
				"figma",
				"figjam"
			],
			"networkAccess": {
				"allowedDomains": [
					"none"
				]
			}
		}
	}
}
