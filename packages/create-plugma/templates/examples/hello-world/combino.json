{
	"meta": {
		"ui": true,
		"type": "plugin",
		"frameworks": ["svelte", "vue", "react"],
		"description": "A blank plugin for you to start from scratch"
	},
	"include": [
		"../../base",
        {
            "source": "../../frameworks/<%= framework %>/components",
            "target": "src/ui/components"
        },
        {
            "source": "../../frameworks/<%= framework %>/assets",
            "target": "src/ui/assets"
        }
	],
	"data": {
		"uiDir": "src/ui",
		"mainDir": "src/main"
	}
}