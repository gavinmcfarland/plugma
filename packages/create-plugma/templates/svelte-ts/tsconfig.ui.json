{
	"extends": ["./tsconfig.base.json", "@tsconfig/svelte/tsconfig.json"],
	"compilerOptions": {
		"resolveJsonModule": true,
		"outDir": "dist/ui",
		"paths": {
			"$lib": ["./src/ui/lib"],
			"$lib/*": ["./src/ui/lib/*"]
		}
	},
	"include": ["src/ui/**/*.ts", "src/ui/**/*.js", "src/ui/**/*.svelte"]
}
