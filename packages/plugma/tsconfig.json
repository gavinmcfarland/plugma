{
  "compilerOptions": {
    "esModuleInterop": true,
    "isolatedModules": true,
    "skipLibCheck": true,
    "lib": ["DOM", "esnext"],
    "module": "esnext",
    "target": "esnext",
    "moduleResolution": "node",
    "outDir": "./dist",
    "strict": true,
    "resolveJsonModule": true,
    "types": ["@figma/plugin-typings"]
  },
  "exclude": ["archive", "dist", "node_modules"]
}
